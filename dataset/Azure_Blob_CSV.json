{
	"name": "Azure_Blob_CSV",
	"properties": {
		"linkedServiceName": {
			"referenceName": "Demo Blob Storage",
			"type": "LinkedServiceReference"
		},
		"type": "AzureBlob",
		"typeProperties": {
			"format": {
				"type": "TextFormat",
				"columnDelimiter": ",",
				"rowDelimiter": "",
				"nullValue": "\\N",
				"treatEmptyAsNull": true,
				"skipLineCount": 0,
				"firstRowAsHeader": true
			},
			"fileName": "forUpdate.csv",
			"folderPath": "files"
		}
	}
}