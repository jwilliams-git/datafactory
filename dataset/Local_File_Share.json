{
	"name": "Local_File_Share",
	"properties": {
		"linkedServiceName": {
			"referenceName": "local_machine_linked_sevice",
			"type": "LinkedServiceReference"
		},
		"type": "FileShare",
		"typeProperties": {
			"format": {
				"type": "TextFormat",
				"columnDelimiter": ",",
				"rowDelimiter": "",
				"nullValue": "\\N",
				"treatEmptyAsNull": true,
				"skipLineCount": 0,
				"firstRowAsHeader": true
			},
			"fileName": {
				"value": "@concat(utcnow(),'.csv')",
				"type": "Expression"
			},
			"folderPath": {
				"value": "files",
				"type": "Expression"
			}
		}
	}
}