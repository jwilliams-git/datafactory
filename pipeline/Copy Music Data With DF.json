{
	"name": "Copy Music Data With DF",
	"properties": {
		"activities": [
			{
				"name": "Lookup Data",
				"type": "Lookup",
				"policy": {
					"timeout": "0.00:1:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "AzureTableSource",
						"azureTableSourceQuery": "Processed eq false",
						"azureTableSourceIgnoreTableNotFound": false
					},
					"dataset": {
						"referenceName": "Data_Factory_Demo_Data_Set",
						"type": "DatasetReference"
					},
					"firstRowOnly": true
				}
			},
			{
				"name": "Copy Data To Blob JSON",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Lookup Data",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "AzureTableSource",
						"azureTableSourceQuery": "Processed eq false",
						"azureTableSourceIgnoreTableNotFound": false
					},
					"sink": {
						"type": "BlobSink"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "Data_Factory_Demo_Data_Set",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureBlob",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "Copy Data To Blob CSV",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Copy Data To Blob JSON",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "AzureTableSource",
						"azureTableSourceQuery": "Processed eq false",
						"azureTableSourceIgnoreTableNotFound": false
					},
					"sink": {
						"type": "BlobSink"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "Data_Factory_Demo_Data_Set",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Azure_Blob_CSV",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Activities"
		}
	}
}