{
	"name": "Copy Data",
	"properties": {
		"activities": [
			{
				"name": "Copy Data1",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "WebSource"
					},
					"sink": {
						"type": "AzureTableSink",
						"azureTableInsertType": "replace",
						"writeBatchSize": 10000
					},
					"enableStaging": false
				},
				"outputs": [
					{
						"referenceName": "AzureTable1",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "For Each File",
				"type": "ForEach",
				"typeProperties": {
					"activities": [
						{
							"name": "Copy Files",
							"type": "Copy",
